@using Microsoft.AspNetCore.Components

<NavLink href="@Href">@Text</NavLink>

@code {
    [Parameter]
    public string Href { get; set; } = "#";

    [Parameter]
    public string Text { get; set; } = "Link";

    [Parameter] 
    public string Class { get; set; } = "nav-link";

    [Parameter]
    public string AriaLabel { get; set; } = "Link";

}